<!DOCTYPE html>
<html lang="en">
<%- include('shared/head') %>
<body>
    <section>
        <h2>Gatos</h2>

        <a href="/admin/gatos/inserir">Inserir Gato</a>
        
        <% for(let gato of gatos){ %>
            <div>
                <p>Nome: <%= gato.nome%></p>
                <img src="/static/<%= gato.img %>">
                <hr>
            </div>
        <% } %>
    
    </section>
</body>
</html>